<% include ../partials/header %>
<br>
 <h2>Sorter By Cheapest price Products</h2>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Tittle</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody id="myTable">
     <% products.forEach(function(product){ %>
        <tr>
            <td scope="row"><%= product.id %></td>
            <td><%= product.title %></td>
            <td><%= product.price %></td>
            
        </tr>
      <% }); %>
    </tbody>
</table>





